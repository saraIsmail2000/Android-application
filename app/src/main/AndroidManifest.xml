<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.bloodbanking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".MyRequests" />
        <activity android:name=".ShowRequest" />
        <activity android:name=".Donations" />
        <activity android:name=".MyInfo" />
        <activity android:name=".SignUp" />
        <activity android:name=".Donate" />
        <activity android:name=".MakeRequest" />
        <activity android:name=".Requests" />
        <activity android:name=".BloodTypes" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".FirstPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>